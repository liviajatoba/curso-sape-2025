FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1 0 0);

boundaryField
{
    entrada
    {
        type            fixedValue;
        value           uniform (1 0 0);
    }
    saida
    {
        type            zeroGradient;
    }
    cilindro
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    planoSim1
    {
        type            symmetryPlane;
    }
    planoSim2
    {
        type            symmetryPlane;
    }
    fundo
    {
        type            empty;
    }
    frente
    {
        type            empty;
    }
}


